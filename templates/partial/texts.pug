// text1 <b>bold1</b> text2 <b>bold2</b> text3
mixin btext(text1, bold1, text2, bold2, text3, bold3)
  if text1
    | #{text1}
  if bold1
    if text1
      b &nbsp;#{bold1}
    else
      b=bold1
  if text2
    |&nbsp;#{text2}
  if bold2
    b &nbsp;#{bold2}
  if text3
    |&nbsp;#{text3}
  if bold3
    b &nbsp;#{bold3}


// text1 <strong><b>sbold1</b></strong> text2 <strong><b>sbold2</b></strong> text3
mixin sbtext(text1, sbold1, text2, sbold2, text3, sbold3)
  if text1
    | #{text1}
  if sbold1
    if text1
      strong
        b &nbsp;#{sbold1}
    else
      strong
        b=sbold1
  if text2
    |&nbsp;#{text2}
  if sbold2
    strong 
      b &nbsp;#{sbold2}
  if text3
    |&nbsp;#{text3}
  if sbold3
    strong
      b &nbsp;#{sbold3}


mixin strongbold(text)
  strong
    b=text