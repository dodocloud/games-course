// text1 <strong>strong1</strong> text2 <strong>strong2</strong> text3
mixin strtext(text1, strong1, text2, strong2, text3, strong3)
  if text1
    | #{text1}
  if strong1
    if text1
      strong &nbsp;#{strong1}
    else
      strong=strong1
  if text2
    |&nbsp;#{text2}
  if strong2
    strong &nbsp;#{strong2}
  if text3
    |&nbsp;#{text3}
  if strong3
    strong &nbsp;#{strong3}

mixin strtext2(text1, strong1, text2, strong2, text3, strong3)
  if text1
    | #{text1}
  if strong1
    if text1
      strong(class="highlight-sec") &nbsp;#{strong1}
    else
      strong(class="highlight-sec")=strong1
  if text2
    |&nbsp;#{text2}
  if strong2
    strong(class="highlight-sec") &nbsp;#{strong2}
  if text3
    |&nbsp;#{text3}
  if strong3
    strong(class="highlight-sec") &nbsp;#{strong3}

